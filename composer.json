{
    "name": "phpp/compiler",
    "description": "PHP 'Pi' Compiler - PHP Transpiler",
    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "PiCompiler\\": "src"
        }
    },

    "require": {
        "symfony/console": "^3.3",
        "nette/di": "^2.4",
        "tracy/tracy": "^2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "slevomat/coding-standard": "^4.0",
        "nikic/php-parser": "^4.0",
        "nette/utils": "^2.4",
        "nette/tokenizer": "^2.3",
        "nette/neon": "^2.4",
        "nette/bootstrap": "^2.4"
    },
    "require-dev": {
        "codeception/codeception": "^2.3"
    }
}
